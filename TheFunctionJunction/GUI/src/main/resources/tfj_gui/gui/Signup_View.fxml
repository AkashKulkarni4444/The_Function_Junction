<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tfj_gui.gui.Signup_View_Controller">
   <children>

      <Label layoutX="480.0" layoutY="40.0" text="New Sign Up" textAlignment="CENTER" textFill="#195190FF">
         <font>
            <Font name="Arial Black" size="21.0" />
         </font>
      </Label>
      <Label layoutX="68.0" layoutY="147.0" text="First Name" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="FirstNameTF" layoutX="214.0" layoutY="145.0" prefHeight="26.0" prefWidth="150.0" promptText="Enter your name here" />
      <Label layoutX="68.0" layoutY="202.0" text="Last Name" textAlignment="CENTER" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="LastNameTF" layoutX="214.0" layoutY="200.0" promptText="Enter your name here" />
      <Label layoutX="68.0" layoutY="259.0" text="Email ID" textAlignment="CENTER" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="EmailIdTF" layoutX="214.0" layoutY="257.0" promptText="Enter your email id" />
      <Label layoutX="68.0" layoutY="314.0" text="Phone Number" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="PhoneNumberTF" layoutX="215.0" layoutY="312.0" prefHeight="26.0" prefWidth="158.0" promptText="Enter your contact number" />
      <Button fx:id="SignUpFinalButton" layoutX="504.0" layoutY="447.0" mnemonicParsing="false" onAction="#SignUpFinalButtonClicked" text="Sign Up" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <CheckBox fx:id="ConfirmDetailsCheckbox" layoutX="448.0" layoutY="398.0" mnemonicParsing="false" text="Confirm your details" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></CheckBox>
      <DatePicker fx:id="DOBTF" layoutX="772.0" layoutY="146.0" onAction="#getDateFromPicker" prefHeight="26.0" prefWidth="130.0" promptText="Select your DOB" />
      <Label layoutX="650.0" layoutY="146.0" text="Date of Birth" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="681.0" layoutY="257.0" text="Password" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <PasswordField fx:id="PasswordTF" layoutX="772.0" layoutY="259.0" promptText="Enter your password" />
      <TextFlow layoutY="1.0" prefHeight="100.0" prefWidth="1080.0">
         <effect>
            <ColorAdjust />
         </effect>
      </TextFlow>
      <Label layoutX="686.0" layoutY="202.0" text="Login Id" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="LoginIdTF" layoutX="772.0" layoutY="202.0" promptText="Login ID" />
      <Button fx:id="AvailableButton" layoutX="944.0" layoutY="194.0" mnemonicParsing="false" onAction="#AvailableButtonClicked" text="Availability?" textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="LoginButton" layoutX="894.0" layoutY="594.0" mnemonicParsing="false" onAction="#LoginButtonClicked" text="Login">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="794.0" layoutY="568.0" text="Already a member? Login Instead." textFill="#195190">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="AvailabilityLabel" layoutX="951.0" layoutY="240.0" textFill="#195190" />
   </children>
   <effect>
      <ColorAdjust brightness="-0.17" />
   </effect>
</AnchorPane>
